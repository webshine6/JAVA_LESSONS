package task2;

public class GsmDemo {

	public static void main(String[] args) {
		// caller gsm number
		GSM gsm1 = new GSM();
		gsm1.insertSimCard("08abcs");

		// receiver gsm number
		GSM gsm2 = new GSM();
		gsm2.insertSimCard("0892563352");

		// make a call
		gsm1.call(gsm2, 5);

		gsm1.printInfoForLastOutgoingCall();
		gsm2.getSumForCall();

		// gsm2.printInfoForLastIncomingCall();

	}

}
