package naselo;

public class Sheep {

	static Peasant ovchar;

	String name;
	int age;
	boolean hasZvunche;

	void makeSound() {
		System.out.println("BeeeEeeEe");

		if (hasZvunche) {
			System.out.println("Dzun-dzun");
		}
	}

	static int kolkoPuloveraMogaDaNapravq(int broiOvche) {
		return (int)(broiOvche * 1.5);
	}
}
